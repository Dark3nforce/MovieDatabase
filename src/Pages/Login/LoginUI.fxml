<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane fx:id="rootPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Pages.Login.LoginController">
   <children>
      <GridPane alignment="CENTER" layoutX="101.0" layoutY="48.0" prefHeight="283.0" prefWidth="399.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="216.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="216.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <TextField fx:id="usernameTF" promptText="Email" GridPane.columnSpan="2" GridPane.rowIndex="1" />
            <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#loginBtn_Pressed" prefHeight="27.0" prefWidth="239.0" text="Pages.Login" textFill="RED" GridPane.columnSpan="2" GridPane.rowIndex="3" />
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="30.0" prefWidth="398.0" text="Crunchy Tomatoes Movie Database" textAlignment="CENTER" textFill="RED" GridPane.columnSpan="2">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#createAccountBtn_Pressed" prefHeight="27.0" prefWidth="239.0" text="Create Account" textFill="RED" GridPane.columnSpan="2" GridPane.rowIndex="4" />
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#oneTimeBtn_Pressed" prefHeight="27.0" prefWidth="239.0" text="One-time User" textFill="RED" GridPane.columnSpan="2" GridPane.rowIndex="5" />
            <Label fx:id="lblLoginFail" alignment="CENTER" prefHeight="31.0" prefWidth="200.0" text="Pages.Login Failed" textFill="RED" visible="false" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <PasswordField fx:id="passwordTF" prefHeight="27.0" prefWidth="259.0" promptText="Password" GridPane.columnSpan="2" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
